{
  "name": "vue-flowy",
  "version": "0.1.2",
  "scripts": {
    "build": "vue-cli-service build --target lib src/main.js",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode development --name vue-flowy --watch --target lib src/main.js",
    "precommit": "lint-staged",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "yarn test:unit && yarn test:e2e"
  },
  "dependencies": {
    "d3": "^5.4.0",
    "dagre-d3-renderer": "^0.5.8",
    "graphlibrary": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.11",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.11",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "@vue/eslint-config-prettier": "^3.0.0-beta.11",
    "@vue/eslint-config-typescript": "^3.0.0-beta.11",
    "@vue/test-utils": "^1.0.0-beta.16",
    "debug": "^3.1.0",
    "jest": "^22.4.3",
    "node-sass": "^4.9.0",
    "prettier-eslint-cli": "^4.7.1",
    "sass-loader": "^7.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-eslint-parser": "^2.0.3"
  },
  "module": "dist/vue-flowy.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Patcher56/vue-flowy.git"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/vue-flowy.umd.js",
  "author": "Patrick Kuen",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "keywords": [
    "flowcharts",
    "diagrams",
    "vuejs",
    "component"
  ],
  "description": "Flowcharts with vuejs on the highest standards"
}
