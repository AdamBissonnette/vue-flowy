{
  "name": "vue-flowy",
  "version": "0.1.2",
  "scripts": {
    "build": "vue-cli-service build --target lib src/main.ts",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode development --name vue-flowy --watch --target lib src/main.ts"
  },
  "dependencies": {
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.0.0"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.11",
    "debug": "^3.1.0",
    "lint-staged": "^7.1.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue": "^2.5.16"
  },
  "module": "dist/vue-flowy.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Patcher56/vue-flowy.git"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/vue-flowy.umd.js",
  "author": "Patrick Kuen",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "keywords": [
    "flowcharts",
    "diagrams",
    "vuejs",
    "component"
  ],
  "description": "Flowcharts with vuejs on the highest standards"
}
